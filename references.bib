	journal = {Language},
	author = {Culicover, Peter W. and Rochemont, Michael},
	year = {1983},
	note = {Publisher: Linguistic Society of America},
	pages = {123--165},
}

@article{raymond_organization_1967,
	title = {the {Organization} of {Computations} for {Uniform} {Recurrence} {Equations}},
	volume = {14},
	abstract = {A set of equations in the q u a n t i t i e s a{\textasciitilde}(p), where i = 1, 2, ... , m and p ranges over a set R of lattice points in n-space, is called a system of uniform recurrence equations if the following p r o p e r t y holds: If p and q are in R and w is an integer n-vector, then a{\textasciitilde}(p) depends directly on aj(p - w) if and only if ai(q) depends directly on a{\textasciitilde}(q - w). Finite-difference approximations to systems of partial differential equations typically lead to such recurrence equations. The structure of such a system is specified by a dependence graph G having m vertices, in which the directed edges are labeled with integer n-vectors. For certain choices of the set R, necessary and sufficient conditions on G are given for the existence of a schedule to compute all the quantities at(p) explicitly from their defining equations. Properties of such schedules, such as the degree to which c o m p u t a t i o n can proceed "in parallel," are characterized. These characterizations depend on a certain iterative decomposition of a dependence graph into subgraphs. Analogous results concerning implicit schedules are also given.},
	language = {en},
	number = {3},
	journal = {Journal of the Association for Computing Machinery},
	author = {Raymond, Richard M Karp and Miller, E and Winograd, Shmuel},
	year = {1967},
	pages = {28},
}

@misc{alice_maz_priesthood_2017,
	title = {A {Priesthood} of {Programmers}},
	url = {https://jacobitemag.com/2017/12/05/a-priesthood-of-programmers/},
