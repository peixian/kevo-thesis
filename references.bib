	volume = {28},
	issn = {01636804},
	doi = {10.1109/35.57696},
	abstract = {The characteristics of meteor burst communications are described{\textbackslash}nand compared to those of HF and satellite systems. The two generic types{\textbackslash}nof MB systems, broadcast and channel-probing systems, are explained. In{\textbackslash}nsystems using the broadcast protocol, the transmitter knows nothing{\textbackslash}nabout the occurrence of bursts or trails and transmits continuously for{\textbackslash}na sufficient duration to permit all recipients to receive the{\textbackslash}nmessage/data with the desired probability. For practical purposes, the{\textbackslash}noptimum burst or block duration for such systems is around 100-200 ms,{\textbackslash}nbased on the characteristics of the predominant trails. Channel-probing{\textbackslash}nMB systems are more common and are commercially available in several{\textbackslash}nversions. Here the transmitter repeatedly sends a short (typically 5-20{\textbackslash}nms) probe signal until it receives a similarly short message response,{\textbackslash}nindicating the availability of a suitable trail. A third, intermediate{\textbackslash}ntype of protocol with hybrid features does not probe the channel with a{\textbackslash}nshort signal, but starts repeatedly transmitting the first packet, in{\textbackslash}neffect using it as a probe signal. Fundamental design issues and/or{\textbackslash}nparameters of specific relevance to MB communication systems are{\textbackslash}ndiscussed. Some historical and more recent MB systems are examined},
	number = {9},
	journal = {IEEE Communications Magazine},
	author = {Yavuz, Davras},
	year = {1990},
	note = {ISBN: 089006444X},
	pages = {40--48},
}

@article{beard_eliminating_2017,
	title = {Eliminating dark bandwidth: {A} data-centric view of scalable, efficient performance, post-moore},
	volume = {10524 LNCS},
	issn = {16113349},
	doi = {10.1007/978-3-319-67630-2_9},
	abstract = {Most of computing research has focused on the computing technologies themselves versus how full systems make use of them (e.g., memory fabric, interconnect, software, and compute elements combined). Technologists have largely failed to look at the compute system as a whole, instead optimizing subsystems mostly in isolation. The result, for example, is that systems are built where applications can only ask for a fixed multiple of data (e.g., 64-bytes from DRAM), even if what is required is far less. This is efficient from a hardware interface perspec-tive, however, it results in consuming valuable bandwidth that is never utilized by the core; this hidden bandwidth is effectively dark to the sys-tem. The causes of dark bandwidth are systemic, built into the very core of our virtual memory abstractions and memory interfaces. Continued focus on newer, revolutionary memory technologies to improve surface performance characteristics without a systems focus on reducing data movement will simply push this problem off onto future systems. This paper examines the problem of dark bandwidth and offers a holistic ap-proach to reduce overall data movement within future compute systems. 1 The Problem Computation is typically not the bottleneck that it once was. Computing itself is faster and less hungry (in terms of energy [7]) than the memory and interconnect that supply the data for computation. Much computing research has focused on providing efficient compute, resulting in the compute cores found in today's CPU sockets. The aforementioned compute devices perform very well for SPEC [4] and LINPACK [3] workloads, however, the compute devices and accompanying sub-systems optimized for these workloads do not necessarily perform well for the applications executed by many HPC [2] and big data systems [12]. Two things are clear: first, architecture researchers have produced a myriad of ways to compute things efficiently, and second, research into ways to feed compute has not kept up. The technical community has been working on a false thesis: that compute systems efficiently utilize the bandwidth provided to the core by the memory subsystem.},
	journal = {Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
	author = {Beard, Jonathan C. and Randall, Joshua},
	year = {2017},
	note = {ISBN: 9783319676296},
	pages = {106--114},
}

