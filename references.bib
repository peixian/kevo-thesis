@article{stancil_demonstration_2012,
	title = {Demonstration of {Communication} using {Neutrinos}},
	issn = {0217-7323},
	url = {http://arxiv.org/abs/1203.2847%0Ahttp://dx.doi.org/10.1142/S0217732312500770},
