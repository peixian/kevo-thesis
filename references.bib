@article{brandt_real_2011,
	title = {Real time, time series forecasting of inter- and intra-state political conflict},
	volume = {28},
	issn = {07388942},
	url = {http://journals.sagepub.com/doi/10.1177/0738894210388125},
	doi = {10.1177/0738894210388125},
	abstract = {We propose a framework for forecasting and analyzing regional and international conflicts. It generates forecasts that (1) are accurate but account for uncertainty, (2) are produced in (near) real time, (3) capture actors’ simultaneous behaviors, (4) incorporate prior beliefs, and (5) generate policy contingent forecasts. We combine the CAMEO event-coding framework with Markov-switching and Bayesian vector autoregression models to meet these goals. Our example produces a series of forecasts for material conflict between the Israelis and Palestinians for 2010. Our forecast is that the level of material conflict between these belligerents will increase in 2010, compared to 2009.},
