@article{simi_why_2018,
	title = {Why {Radicalization} {Fails}: {Barriers} to {Mass} {Casualty} {Terrorism}},
	issn = {0954-6553, 1556-1836},
	shorttitle = {Why {Radicalization} {Fails}},
	url = {https://www.tandfonline.com/doi/full/10.1080/09546553.2017.1409212},
	doi = {10.1080/09546553.2017.1409212},
	abstract = {Few issues have garnered as much attention in recent years as the topic of violent extremism (VE). Although substantial attention has been devoted to investigating the radicalization process, few scholars have examined the obstacles that hinder VE radicalization. Based on in-depth life history interviews, the current study examines ﬁve types of barriers that hinder radicalization toward mass casualty violence (MCV): mass casualty violence as counter-productive; preference toward interpersonal violence, changes in focus and availability; internal organizational conﬂict; and moral apprehension. In general, we address each barrier’s unique contribution to hindering the likelihood of MCV. Finally, we discuss how our ﬁndings could be used as part of initiatives aimed at countering violent extremism (CVE).},
