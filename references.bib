	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/nana.12684},
	keywords = {Economic Nationalism, Everyday Nationalism, Local Nationalism, Nationalism From Below, artificial intelligence, everyday nationhood, nationalism, social change},
}

